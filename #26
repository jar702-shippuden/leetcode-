class Solution(object):
    def removeDuplicates(self, nums):
        #nums ascending order remove duplicates, order same
        # k-1 of nums should contain order 
        #create two points
        n = len(nums)
        j = 1

        for i in range(1, n):
            if nums[i] != nums[i - 1]:
                nums[j] = nums[i]
                j += 1

        return j
        #time = O(n)
        #space = O(1)


