class Solution(object):
    def removeDuplicates(self, nums):
        j = 1 # first variable
        count = 1 #2nd function determines occurences 1-2
        n = len(nums) #scans whole length

        for i in range (1, n): 
            if nums[i] == nums[i-1]:
                count += 1
            else:
                count = 1
            if count <= 2:
                nums[j] = nums[i]
                j += 1
        return j
        
